<document order="23">
  <properties>
    <title>HasManyAttribute</title>
  </properties>

  <body>

<intro>
<p>
 Maps a one to many association. 
</p>


</intro>

<section>
	<title>Example</title>
	
	<pre format="cs">public class Blog : ActiveRecordBase
{
    ...
    
    [HasMany(typeof(Post), RelationType.Bag, Key="Posts", Table="Posts", Column="blogid")]
    public IList Posts
    {
        get { return _posts; }
        set { _posts = value; }
    }
</pre>
</section>

