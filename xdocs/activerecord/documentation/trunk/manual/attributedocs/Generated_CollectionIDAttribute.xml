  <properties>
    <title>CollectionIDAttribute</title>
  </properties>

  <body>

<intro>
<p>
 Used for a collection that requires a collection id. 
</p>


</intro>

<section>
	<title>Example</title>
	
	<pre format="cs">public class Blog : ActiveRecordBase
{
    ...
    
    [HasManyAndBelongs/HasMany]
    [CollectionIDAttribute(CollectionIDAttribute.Native)]
    public int Id
    {
        get { return _id; }
        set { _id = value; }
    }
</pre>
</section>

